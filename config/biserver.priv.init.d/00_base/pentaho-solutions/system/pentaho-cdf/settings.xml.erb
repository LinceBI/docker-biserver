<?xml version="1.0" encoding="UTF-8"?>
<settings>

	<cache>false</cache>
	<cache-messages>true</cache-messages>
	<max-age>2628001</max-age>
	<resources>
		<downloadable-formats>css,js,html,htm,png,jpg,jpeg,gif,swf,otf,woff,ico,zip,xml,properties</downloadable-formats>
	</resources>

	<!--
	Flag indicating if the Hibernate database is available (not available in AGILE-BI and cause issues).
	This flag allows agile-bi to override the default setting of true
	Valid values are: true | false
	-->
	<hibernate-available>true</hibernate-available>

	<!--
	Flag indicating if Dashboard.context should assume the legacy structure, including deprecated
	attributes such as: solution, path, file, fullPath, isAdmin
	Accepted values are: true | false
	-->
	<legacy-dashboard-context>false</legacy-dashboard-context>

	<!--
	Flag indicating cross-origin resource sharing
	Accepted values are: true | false
	-->
	<allow-cross-domain-resources><%= ENV['CORS_REQUESTS_ALLOWED'] %></allow-cross-domain-resources>

	<!-- A comma separated list of allowed domains for a cross-origin resource sharing -->
	<cross-domain-resources-whitelist><%= ENV['CORS_REQUESTS_ALLOWED_DOMAINS'] %></cross-domain-resources-whitelist>

</settings>
