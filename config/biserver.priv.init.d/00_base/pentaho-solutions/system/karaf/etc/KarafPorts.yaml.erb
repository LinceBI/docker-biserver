Service:
  - serviceName: karaf
    serviceDescription: Karaf service ports

ServerPort:
  - id: KARAF_PORT
    property: karaf.port
    friendlyName: Karaf Port
    startPort: <%= ENV['KARAF_STARTPORT'] %>
    endPort: <%= ENV['KARAF_ENDPORT'] %>
    serviceName: karaf

  - id: OSGI_SERVICE_PORT
    property: org.pentaho.osgi.service.http.port
    friendlyName: OSGI Service Port
    startPort: <%= ENV['OSGI_SERVICE_STARTPORT'] %>
    endPort: <%= ENV['OSGI_SERVICE_ENDPORT'] %>
    serviceName: karaf
